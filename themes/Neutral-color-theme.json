{
  "name": "Neutral",
  "colors": {
    // Sidebar
    "activityBar.activeBorder": "#aaa",
    "activityBar.activeFocusBorder": "#aaa",
    "activityBar.background": "#0D0D0D",
    "activityBar.foreground": "#aaa",
    "activityBar.inactiveForeground": "#444",
    "activityBarBadge.background": "#5b0096",
    "activityBarBadge.foreground": "#ccc",
    "button.background": "#444",
    "focusBorder": "#0D0D0D",
    "titleBar.activeBackground": "#0D0D0D",
    "titleBar.inactiveBackground": "#0D0D0D",
    "titleBar.border": "#3e3d3db6",
    "list.activeSelectionBackground": "#333",
    "list.hoverBackground": "#0D0D0D",
    "list.hoverForeground": "#fff",
    "progressBar.background": "#aaa",
    "selection.background": "#C984FF",
    "sideBar.border": "#3e3d3db6",
    "sideBar.background": "#0D0D0D",
    "sideBarSectionHeader.background": "#0D0D0D",
    "sideBarTitle.foreground": "#bbb",
    "textLink.foreground": "#C984FF",
    // Editor
    "editor.background": "#0D0D0D",
    "editor.findMatchBackground": "#555",
    "editor.findMatchHighlightBackground": "#333",
    "editor.foreground": "#ccc",
    "editor.lineHighlightBackground": "#222",
    "editor.selectionBackground": "#333",
    "editor.selectionHighlightBackground": "#333",
    "editorLineNumber.foreground": "#3E3D3D",
    "editorBracketHighlight.foreground1": "#72a6bd",
    "editorBracketHighlight.foreground2": "#72a6bd",
    "editorBracketHighlight.foreground3": "#72a6bd",
    // Tab
    // "tab.activeBorder": "#3E3D3D",
    // "tab.border": "#3e3d3db6",
    "tab.inactiveBackground": "#0D0D0D",
    "tab.activeBorderTop": "#8E876C",
    // Minimap
    "editorGutter.addedBackground": "#73C991",
    "editorGutter.deletedBackground": "#F88070",
    "editorGutter.modifiedBackground": "#E2C08D",
    "minimap.selectionHighlight": "#C984FF",
    "minimapGutter.addedBackground": "#73C991",
    "minimapGutter.deletedBackground": "#F88070",
    "minimapGutter.modifiedBackground": "#E2C08D",
  },
  "tokenColors": [
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#7a202f"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666"
      }
    },
    {
      "name": "Keyword, Operator",
      "scope": [
        "constant.other.color",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.operator",
        "entity.other.attribute-name",
        "support.type.primitive",
        "entity.name.type",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#DFD8D1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword, Operator italic",
      "scope": [
        "constant.language",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.function",
        "keyword.operator.expression",
        "meta.separator",
        "variable.language.this",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#a2a2a2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "usual",
      "scope": [
        "meta.object",
        "meta.jsx.children",
        "support.type.property-name",
        "variable.object.property",
        "variable.other.property",
        "variable.other.object",
        "variable.other.readwrite",
        "variable.other",
        "meta.embedded.expression",
        "meta.definition",
        "entity.name.tag.yaml",
        "support.type.property-name.json",
        "meta.parameters",
      ],
      "settings": {
        "foreground": "#d0d0d0",
        "fontStyle": ""
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block",
        "entity.name",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "entity.other.inherited-class",
        "variable.other.readwrite.alias",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.constant.property",
        "variable.parameter",
        "meta.block",
        "constant.language.import-export-all",
        "support.type.exception",
        "entity.name.type.interface",
        "markup.heading",
        "punctuation.separator.key-value",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#fff",
        "fontStyle": ""
      }
    },
    {
      "name": "Block Level Variables italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#fff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#fff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "African Violet",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other",
        "meta.tag",
        "punctuation.decorator",
        "entity.name.type.class",
        "constant.other.character-class.regexp"
      ],
      "settings": {
        "foreground": "#d9baae",
        // "foreground": "#B2A48F",
        // "foreground": "#a1b693",
        
        "fontStyle": "bold",
      }
    },
    {
      "name": "African Violet italic",
      "scope": [
        "markup.quote",
        "keyword.control.pseudo-method",
      ],
      "settings": {
        "foreground": "#C984FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Burnt Sienna",
      "scope": [
        "constant.numeric",
        "punctuation.definition.list",
        "constant.character.entity",
        "meta.image.inline",
        "constant.other.symbol",
        "source.js constant.other.object.key.js string.unquoted.label.js",
        "constant.character.escape",
        "keyword.operator.quantifier.regexp"
      ],
      "settings": {
        "foreground": "#cd9878",
        "fontStyle": ""
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        // "foreground": "#B2A48F",
        "foreground": "#a1b693",
        "fontStyle": ""
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
  ]
}